#!/bin/zsh

. $XDG_CONFIG_HOME/zsh/functions.zsh

function help() {
	print_error "There is no helping you."
}

function poop() {
	print_message "This is a duece."
}

{ eval "$1" } 2> tmp
if [ "$(cat tmp | grep 'command not found: ')" != "" ]
then
	print_error "Command not found"
else
	print_message "Executed succesfully"
fi
rm tmp
